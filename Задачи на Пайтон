# Ð¾Ð±ÑŠÑÐ²Ð»ÐµÐ½Ð¸Ðµ Ñ„ÑƒÐ½ÐºÑ†Ð¸Ð¸

def is_palindrome(text):

    t = ''

    flag = False

    text = text.lower()

    for i in range(len(text)):

        if 'Ð°' <= text[i] <= 'Ñ' or 'a' <= text[i] <= 'z':

            t += text[i]

    if t == t[::-1]:

        return True

    else:

        return False

        
